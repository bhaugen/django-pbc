<style type="text/css">

/* begin css tabs */

ul#tabnav 
{ /* general settings */
	text-align: left; /* set to left, right or center */
	margin: 1em 0 2em 0; /* set margins as desired */
	font: bold 11px verdana, arial, sans-serif; /* set font as desired */
	border-bottom: 1px solid #006699; /* set border COLOR as desired */
	list-style-type: none;
	padding: 1px 10px 3px 10px; /* THIRD number must change with respect to padding-top (X) below */
}

ul#tabnav li 
{ /* do not change */
display: inline;
}

div.index li.index, div.inventory-update li.inventory-update, div.order-update li.order-update, div.order-table li.order-table, div.delivery-update li.delivery-update, div.deliveries li.deliveries, div.producer-payments li.producer-payments, div.payment-update li.payment-update, div.invoices li.invoices, div.help li.help, div.plan-update li.plan-update, div.statements li.statements
{ /* settings for selected tab */
	border-bottom: 1px solid #fff; /* set border color to page background color */
	background-color: #fff !important; /* set background color to match above border color */
}

div.index li.index a, div.inventory-update li.inventory-update a, div.order-update li.order-update a, div.order-table li.order-table a, div.delivery-update li.delivery-update a, div.deliveries li.deliveries a, div.producer-payments li.producer-payments a, div.payment-update li.payment-update a, div.invoices li.invoices a, div.help li.help a, div.plan-update li.plan-update a, div.statements li.statements a
{ /* settings for selected tab link */
	background-color: #fff  !important; /* set selected tab background color as desired */
	color: #000; /* set selected tab link color as desired */
	position: relative;
	top: 1px;
	padding-top: 4px; /* must change with respect to padding (X) above and below */
}

ul#tabnav li a 
{ /* settings for all tab links */
	padding: 3px 4px; /* set padding (tab size) as desired; FIRST number must change with respect to padding-top (X) above */
	border: 1px solid #006699; /* set border COLOR as desired; usually matches border color specified in #tabnav */
	background-color: #D0D0D0; /* set unselected tab background color as desired */
	color: #666; /* set unselected tab link color as desired */
	margin-right: 0px; /* set additional spacing between tabs as desired */
	text-decoration: none;
	border-bottom: none;
}

ul#tabnav a:hover 
{ /* settings for hover effect */
	background: #fff; /* set desired hover color */
}

/* end css tabs */
	
</style>

